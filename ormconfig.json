[
  {
    "name": "production",
    "type": "postgres",
    "synchronize": true,
    "logging": true
  },
  {
    "name": "development",
    "type": "sqlite",
    "database": "profi.ru.sqlite.tmp",
    "synchronize": true,
    "logging": true,
    "entities": [
      "src/modules/**/*.model.ts"
    ],
    "migrations": [
      "src/migration/**/*.*"
    ],
    "subscribers": [
      "src/subscriber/**/*.*"
    ]
  }
]
